    "@typescript-eslint/parser": "^5.58.0",
    "autoprefixer": "10.4.13",
    "cssnano": "5.1.15",
    "eslint": "^8.38.0",
    "vite": "4.2.1",
    "vite-plugin-dts": "2.0.2"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,yaml,md}": "eslint --fix"
